CREATE TABLE groceries (id INTEGER PRIMARY KEY, item TEXT, quantity INTEGER, aisle INTEGER);
INSERT INTO groceries VALUES 
(1, "Bananas", 4, 7);
(2, "Peanut Butter", 1, 2);
(3, "Dark Chocolate Bars", 2, 2);
(4, "Ice cream", 1, 12);
(5, "Cherries", 1, 2);
(6, "Chocolate syrup", 0, 4);

SELECT* groceries

SELECT SUM(quantity)
FROM groceries 
GROUP BY aisle;
